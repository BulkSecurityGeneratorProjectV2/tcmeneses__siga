<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<data-sources />
	<form-beans>
		<form-bean name="DpPessoaForm" type="br.gov.jfrj.struts.form.DpPessoaForm"></form-bean>
		<form-bean name="DpLotacaoForm" type="br.gov.jfrj.struts.form.DpLotacaoForm"></form-bean>
		<form-bean name="CadastroEmpresaForm" type="br.gov.jfrj.struts.form.CadastroEmpresaForm" />
		<form-bean name="ExDocumentoForm" type="br.gov.jfrj.struts.form.ExDocumentoForm"></form-bean>
		<form-bean name="ExClassificacaoForm" type="br.gov.jfrj.struts.form.ExClassificacaoForm"></form-bean>
		<form-bean name="ExMovimentacaoForm" type="br.gov.jfrj.struts.form.ExMovimentacaoForm"></form-bean>
		<form-bean name="ExTipoDespachoForm" type="br.gov.jfrj.struts.form.ExTipoDespachoForm"></form-bean>
		<form-bean name="CpOrgaoForm" type="br.gov.jfrj.struts.form.CpOrgaoForm"></form-bean>
		
		<form-bean name="ExTipologiaForm" type="br.gov.jfrj.struts.form.ExTipologiaForm"></form-bean>
		
		<form-bean name="UsuarioForm" 
			type="br.gov.jfrj.struts.form.UsuarioForm">
		</form-bean>
		

		
	</form-beans>

	<global-exceptions>

		<exception type="java.lang.Exception" key="csis.erros" scope="request" handler="br.gov.jfrj.struts.action.CsisExceptionHandler" />
	</global-exceptions>

	<!-- =========================================== Global Forward Definitions -->


	<global-forwards>
		<!-- Default forward to "Welcome" action -->
		<!-- Demonstrates using index.jsp to forward -->
		<forward name="inicial" path="/principal.jf" />
		<forward name="errogeral" path="/erroGeral.jf" />
		<!-- 	
			<forward name="autenticado" path="/autenticado.jf" />
		-->
		<forward name="logoff" path="/logoff.jf" />
	</global-forwards>

	<!-- =========================================== Action Mapping Definitions -->

	<action-mappings>

		<action path="/inicial" forward="/paginas/entradas/inicial.jsp" />
		<action path="/logoff" type="br.gov.jfrj.struts.action.Logoff">
			<forward name="sucesso" path="/index.jsp" />
		</action>
		<action path="/administracao" forward="/paginas/entradas/administracao.jsp" />
		<action path="/erroGeral" forward="/paginas/erros/erroGeral.jsp" />

		<!-- Geral -->
		<action path="/css" forward="/paginas/siga.css" />
		<action path="/ajax" forward="/paginas/ajax.js" />
		<action path="/staticJavascript" forward="/paginas/static_javascript.jsp" />
		<action path="/ajax_vazio" forward="/paginas/ajax_vazio.jsp" />
		<action path="/ajax_retorno" forward="/paginas/ajax_retorno.jsp" />
		<action path="/modelo" forward="/paginas/expediente/modelo_teste.jsp" />

		<action path="/principal" type="br.gov.jfrj.struts.action.Principal">
			<forward name="sucesso" path="/paginas/principal.jsp" />
		</action>

		<action path="/pessoa" type="br.gov.jfrj.struts.action.DpPessoaAction" 
			name="DpPessoaForm" scope="request" validate="false"
			parameter="acao" input="/paginas/pessoa_busca.jsp">
			<forward name="busca" path="/paginas/pessoa_busca.jsp" />
			<forward name="sucesso" path="/paginas/principal.jsp" />
			<forward name="ajax_vazio" path="/paginas/ajax_vazio.jsp" />
			<forward name="ajax_retorno" path="/paginas/ajax_retorno.jsp" />
		</action>

		<action path="/lotacao" type="br.gov.jfrj.struts.action.DpLotacaoAction" 
			name="DpLotacaoForm" scope="request" validate="false"
			parameter="acao" input="/paginas/lotacao_busca.jsp">
			<forward name="busca" path="/paginas/lotacao_busca.jsp" />
			<forward name="sucesso" path="/paginas/principal.jsp" />
			<forward name="ajax_vazio" path="/paginas/ajax_vazio.jsp" />
			<forward name="ajax_retorno" path="/paginas/ajax_retorno.jsp" />
		</action>
		
		<action path="/orgao" type="br.gov.jfrj.struts.action.CpOrgaoAction" 
			name="CpOrgaoForm" scope="request" validate="false"
			parameter="acao" input="/paginas/orgao_busca.jsp">
			<forward name="busca" path="/paginas/orgao_busca.jsp" />
			<forward name="sucesso" path="/paginas/principal.jsp" />
			<forward name="ajax_vazio" path="/paginas/ajax_vazio.jsp" />
			<forward name="ajax_retorno" path="/paginas/ajax_retorno.jsp" />
		</action>

		<action path="/tipologia" type="br.gov.jfrj.struts.action.ExTipologiaAction" 
			name="ExTipologiaForm" scope="request" validate="false"
			parameter="acao" input="/paginas/tipologia_busca.jsp">
			<forward name="busca" path="/paginas/tipologia_busca.jsp" />
			<forward name="sucesso" path="/paginas/principal.jsp" />
			<forward name="ajax_vazio" path="/paginas/ajax_vazio.jsp" />
			<forward name="ajax_retorno" path="/paginas/ajax_retorno.jsp" />
		</action>
		
		<action path="/classificacao" type="br.gov.jfrj.struts.action.ExClassificacaoAction" 
			name="ExClassificacaoForm" scope="request" validate="false"
			parameter="acao" input="/paginas/classificacao_busca.jsp">
			<forward name="busca" path="/paginas/classificacao_busca.jsp" />
			<forward name="sucesso" path="/paginas/principal.jsp" />
			<forward name="ajax_vazio" path="/paginas/ajax_vazio.jsp" />
			<forward name="ajax_retorno" path="/paginas/ajax_retorno.jsp" />
		</action>

		<action path="/cabecalho" forward="/paginas/cabecalho.jsp" />
		<action path="/rodape" forward="/paginas/rodape.jsp" />
		<action path="/cabecalho_popup" forward="/paginas/cabecalho_popup.jsp" />
		<action path="/rodape_popup" forward="/paginas/rodape_popup.jsp" />


		<!-- Controle de Expediente -->

		<action path="/expediente/doc"
		        type="br.gov.jfrj.struts.action.ExDocumentoAction" 
		        name="ExDocumentoForm" scope="request" validate="true"
			parameter="acao" input="/paginas/expediente/edita.jsp">
			<forward name="edita" path="/paginas/expediente/edita.jsp" />
			<forward name="gravado" path="/paginas/expediente/exibe.jsp" />
			<forward name="fechado" path="/paginas/expediente/exibe.jsp" />
			<forward name="exibe" path="/paginas/expediente/exibe.jsp" />
			<!-- <forward name="exibe_popup" path="/paginas/expediente/exibe_popup.jsp" /> -->
			<forward name="lista" path="/paginas/expediente/lista.jsp" />
			<forward name="preve" path="/paginas/expediente/preve.jsp" />
			<forward name="processa_modelo" path="/paginas/expediente/processa_modelo.jsp" />
			<forward name="ajax_vazio" path="/paginas/ajax_vazio.jsp" />
			<forward name="ajax_retorno" path="/paginas/ajax_retorno.jsp" />
			<forward name="busca" path="/paginas/expediente/lista.jsp?popup=true" />
			<forward name="anexo" path="/paginas/expediente/doc_anexo.jsp" />
		</action>

		<action path="/expediente/mov" 
		        type="br.gov.jfrj.struts.action.ExMovimentacaoAction" 
		        name="ExMovimentacaoForm" scope="request" validate="false"
			parameter="acao" input="/paginas/expediente/exibe.jsp">
			<forward name="anexa" path="/paginas/expediente/anexa.jsp" />
			<forward name="anexado" path="/paginas/expediente/exibe.jsp" />
			<forward name="anexo" path="/paginas/expediente/mov_anexo.jsp" />
			<forward name="transfere" path="/paginas/expediente/transfere.jsp" />
			<forward name="transferido" path="/paginas/expediente/exibe.jsp" />
			<forward name="assina" path="/paginas/expediente/assina.jsp" />
			<forward name="assinado" path="/paginas/expediente/exibe.jsp" />
			<forward name="verificar_assinatura_ok" path="/paginas/assinatura_ok.jsp" />
			<forward name="verificar_assinatura_erro" path="/paginas/assinatura_erro.jsp" />
			<forward name="vincula" path="/paginas/expediente/vincula.jsp" />
			<forward name="vinculado" path="/paginas/expediente/exibe.jsp" />
			<forward name="desvinculado" path="/paginas/expediente/exibe.jsp" />
			<forward name="cancelado" path="/paginas/expediente/exibe.jsp" />
			<forward name="referencia" path="/paginas/expediente/referencia.jsp" />
			<forward name="referenciado" path="/paginas/expediente/exibe.jsp" />
			
		</action>

		<action path="/tipodespacho/doc" 
		        type="br.gov.jfrj.struts.action.ExTipoDespachoAction" 
		        name="ExTipoDespachoForm" 
		        scope="request" 
		        validate="false"
    			parameter="acao" >
			<forward name="edita" path="/paginas/despacho/editaTipo.jsp" />
			<forward name="lista" path="/paginas/despacho/listaTipos.jsp" />
		</action>

		<action path="/administra/usuario" 
		        type="br.gov.jfrj.struts.action.UsuarioAction" 
		        name="UsuarioForm" 
		        scope="request" 
		        input="/paginas/administracao/trocasenha.jsp"
		        validate="true"
    			parameter="acao" >
			<forward name="listar" path="/paginas/administracao/trocasenha.jsp" />
			<forward name="sucesso" path="/paginas/administracao/senhatrocada.jsp" />
		</action>
		
		<action path="/listagem"
			type="br.gov.jfrj.struts.action.ListaOpcoes">
			<forward name="sucesso"
				path="/paginas/expediente/listaopcoes.jsp" />
		</action>

		<action path="/mod" forward="/paginas/expediente/modelos/pape.jsp" />

	</action-mappings>
	<controller processorClass="br.gov.jfrj.struts.action.CsisRequestProcessor" />

	<message-resources parameter="resource/application"/>

	<!-- =================================================== Validator plugin -->

	<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames" value="/WEB-INF/validator-rules.xml,/WEB-INF/validation.xml" />
	</plug-in>

</struts-config>
