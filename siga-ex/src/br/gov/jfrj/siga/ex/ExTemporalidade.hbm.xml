<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<!-- Created Mon Nov 14 13:33:06 GMT-03:00 2005                         -->
<hibernate-mapping package="br.gov.jfrj.siga.ex">

	<class name="ExTemporalidade" schema="SIGA" table="EX_TEMPORALIDADE">
		<id name="idTemporalidade" column="ID_TEMPORALIDADE"
			type="short">
			<generator class="sequence" >
			<param name="sequence">EX_TEMPORALIDADE_SEQ</param>
			</generator>
		</id>

		<property name="descTemporalidade" column="DESC_TEMPORALIDADE"
			type="string" not-null="true" />
		<property name="permanenciaArquivo" column="PERMANENCIA_ARQUIVO"
			type="integer" />
	</class>

</hibernate-mapping>
